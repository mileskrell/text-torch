<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/deep_orange_300</item>
        <item name="colorPrimaryVariant">@color/deep_orange_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/light_blue_200</item>
        <item name="colorSecondaryVariant">@color/light_blue_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:textColorLink">@color/light_blue_200</item>
        <item name="welcomePageBackgroundColor">@color/blue_900</item>
        <item name="welcomePageLogoColor">@color/blue_900_darkened_by_10_percent</item>
        <item name="permissionsPageBackgroundColor">@color/teal_900</item>
        <item name="permissionsPageLogoColor">@color/teal_900_darkened_by_10_percent</item>
        <item name="analyticsPageBackgroundColor">@color/green_900</item>
        <item name="analyticsPageLogoColor">@color/green_900_darkened_by_10_percent</item>
        <item name="enterAppPageBackgroundColor">@color/deep_orange_900</item>
        <item name="enterAppPageLogoColor">@color/deep_orange_900</item>
        <item name="leftBarColor">@color/red_a100</item>
        <item name="rightBarColor">@color/blue_a100</item>
    </style>
</resources>
